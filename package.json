{
  "name": "discord.js-components-bot",
  "private": true,
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "lint": "prettier --ignore-path .gitignore -c .",
    "format": "prettier --ignore-path .gitignore -w .",
